{
  "name": "musterweg-app",
  "version": "0.1.1-dev",
  "description": "App der Genossenschaft Musterweg",
  "productName": "Musterweg App",
  "author": "Marco Steinacher <marco.steinacher@tegonal.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn run make:tenant-config && quasar dev",
    "dev:watch": "node scripts/watch-config.js & quasar dev",
    "watch:config": "node scripts/watch-config.js",
    "build": "yarn run make:tenant-config && quasar build -m pwa",
    "build:pwa": "yarn run make:tenant-config && quasar build -m pwa",
    "make:tenant-config": "node scripts/make-tenant-config.js",
    "serve": "quasar serve dist/pwa/",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{js,vue}\"",
    "lint:fix": "eslint -c ./eslint.config.js \"./src*/**/*.{js,vue}\" --fix",
    "format": "prettier --write \"**/*.{js,mjs,cjs,vue,scss,css,html,md,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,mjs,cjs,vue,scss,css,html,md,json,yml,yaml}\"",
    "test": "echo \"No test specified\" && exit 0",
    "check": "yarn lint:fix && yarn format",
    "upgrade": "quasar upgrade",
    "upgrade:install": "quasar upgrade --install",
    "prepare": "quasar prepare",
    "optimize:images": "node scripts/optimize-images.js",
    "up": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@fullcalendar/vue3": "^6.1.19",
    "@quasar/extras": "^1.17.0",
    "axios": "^1.11.0",
    "pinia": "^2.3.1",
    "quasar": "^2.18.2",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.21",
    "vue-i18n": "^10.0.8",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@eslint/js": "^9.34.0",
    "@intlify/unplugin-vue-i18n": "^5.3.1",
    "@quasar/app-vite": "^2.3.0",
    "@quasar/cli": "^2.5.0",
    "@quasar/icongenie": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "eslint": "^9.34.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^15.15.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "register-service-worker": "^1.7.2",
    "sharp": "^0.34.3",
    "svgo": "^4.0.0",
    "typescript": "^5.9.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-eslint-parser": "^10.2.0",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0"
  },
  "engines": {
    "node": "^22",
    "yarn": ">= 4"
  },
  "packageManager": "yarn@4.9.4"
}
