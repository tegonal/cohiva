{
  "name": "musterweg-app",
  "version": "1.4.0-SNAPSHOT",
  "description": "App der Genossenschaft Musterweg",
  "productName": "Musterweg App",
  "author": "Marco Steinacher <marco.steinacher@tegonal.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn run setup:dev-config && yarn run validate:config && quasar dev",
    "dev:tenant": "tsx scripts/dev-with-tenant.ts",
    "build": "yarn run validate:config && quasar build -m pwa",
    "build:pwa": "yarn run validate:config && quasar build -m pwa",
    "setup:dev-config": "tsx scripts/setup-dev-config.ts",
    "setup:ci-config": "tsx scripts/setup-ci-config.ts",
    "validate:config": "tsx scripts/validate-config.ts",
    "clean": "rm -rf tenant-config/overlay tenant-config/src tenant-config/public public/manifest.json public/icons public/og-image.png public/twitter-card.png",
    "serve": "quasar serve dist/pwa/",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,vue}\" \"./scripts/**/*.ts\"",
    "lint:fix": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,vue}\" \"./scripts/**/*.ts\" --fix",
    "prettier:fix": "prettier --write \"**/*.{js,ts,mjs,cjs,vue,scss,css,html,md,json,yml,yaml}\"",
    "prettier": "prettier --check \"**/*.{js,ts,mjs,cjs,vue,scss,css,html,md,json,yml,yaml}\"",
    "test": "echo \"No test specified\" && exit 0",
    "type-check": "vue-tsc --noEmit",
    "check": "yarn type-check && yarn lint && yarn prettier",
    "upgrade": "quasar upgrade",
    "upgrade:install": "quasar upgrade --install",
    "up": "yarn upgrade-interactive",
    "self-update": "yarn set version stable && yarn"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@fullcalendar/vue3": "^6.1.19",
    "@khmyznikov/pwa-install": "^0.5.8",
    "@quasar/extras": "^1.17.0",
    "@types/browser-lang": "^0.1.2",
    "axios": "^1.12.2",
    "browser-lang": "^0.2.1",
    "lit": "^3.3.1",
    "oidc-client-ts": "^3.3.0",
    "pinia": "^3.0.4",
    "quasar": "^2.18.5",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.22",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.6",
    "@eslint/js": "^9.39.2",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/cli": "^2.5.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.0.8",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^15.15.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "register-service-worker": "^1.7.2",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.1.1",
    "workbox-build": "^7.4.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": "^24",
    "yarn": "^4.9.4"
  },
  "packageManager": "yarn@4.10.3",
  "resolutions": {
    "qs": "^6.14.1",
    "node-forge": "^1.3.2",
    "vite": "^7.1.11",
    "glob": "^10.5.0",
    "tar": "^7.5.3",
    "js-yaml": "^4.1.1"
  }
}
