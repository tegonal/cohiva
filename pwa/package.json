{
  "name": "musterweg-app",
  "version": "1.3.0-SNAPSHOT",
  "description": "App der Genossenschaft Musterweg",
  "productName": "Musterweg App",
  "author": "Marco Steinacher <marco.steinacher@tegonal.com>",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "yarn run validate:config && yarn run make:tenant-config && quasar dev",
    "dev:watch": "yarn run validate:config && tsx scripts/watch-config.ts & quasar dev",
    "watch:config": "yarn run validate:config && tsx scripts/watch-config.ts",
    "build": "yarn run validate:config && yarn run make:tenant-config && quasar build -m pwa",
    "build:pwa": "yarn run validate:config && yarn run make:tenant-config && quasar build -m pwa",
    "validate:config": "tsx scripts/validate-config.ts",
    "make:tenant-config": "tsx scripts/make-tenant-config.ts",
    "serve": "quasar serve dist/pwa/",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,vue}\" \"./config/**/*.{ts,js}\" \"./scripts/**/*.ts\"",
    "lint:fix": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,vue}\" \"./config/**/*.{ts,js}\" \"./scripts/**/*.ts\" --fix",
    "format": "prettier --write \"**/*.{js,ts,mjs,cjs,vue,scss,css,html,md,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,mjs,cjs,vue,scss,css,html,md,json,yml,yaml}\"",
    "test": "echo \"No test specified\" && exit 0",
    "type-check": "vue-tsc --noEmit",
    "check": "yarn type-check && yarn lint:fix && yarn format",
    "upgrade": "quasar upgrade",
    "upgrade:install": "quasar upgrade --install",
    "prepare": "quasar prepare",
    "optimize:images": "tsx scripts/optimize-images.ts",
    "download:fonts": "tsx scripts/download-roboto.ts",
    "icon:generate": "npx @quasar/icongenie generate -p icongenie-profile.json -o public",
    "up": "yarn upgrade-interactive",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.19",
    "@fullcalendar/list": "^6.1.19",
    "@fullcalendar/timegrid": "^6.1.19",
    "@fullcalendar/vue3": "^6.1.19",
    "@khmyznikov/pwa-install": "^0.5.8",
    "@quasar/extras": "^1.17.0",
    "@types/browser-lang": "^0.1.2",
    "axios": "^1.12.2",
    "browser-lang": "^0.2.1",
    "lit": "^3.3.1",
    "oidc-client-ts": "^3.3.0",
    "pinia": "^3.0.0",
    "quasar": "^2.18.5",
    "tippy.js": "^6.3.7",
    "vue": "^3.5.21",
    "vue-i18n": "^11.1.12",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@eslint/js": "^9.36.0",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@quasar/app-vite": "^2.4.0",
    "@quasar/cli": "^2.5.0",
    "@quasar/icongenie": "^4.0.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "eslint": "^9.36.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-vue": "^10.5.0",
    "globals": "^15.15.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "register-service-worker": "^1.7.2",
    "sharp": "^0.34.4",
    "svgo": "^4.0.0",
    "tsx": "^4.20.5",
    "typescript": "~5.9.2",
    "vite-plugin-imagemin": "^0.6.1",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.7",
    "workbox-build": "^7.3.0",
    "workbox-cacheable-response": "^7.3.0",
    "workbox-core": "^7.3.0",
    "workbox-expiration": "^7.3.0",
    "workbox-precaching": "^7.3.0",
    "workbox-routing": "^7.3.0",
    "workbox-strategies": "^7.3.0",
    "zod": "^4.1.11"
  },
  "engines": {
    "node": "^24",
    "yarn": "^4.9.4"
  },
  "packageManager": "yarn@4.9.4"
}
